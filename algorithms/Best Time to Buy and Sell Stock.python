class Solution(object):
    def maxProfit(self, prices):
        """
        :type prices: List[int]
        :rtype: int
        """
        profitMax=0
        if len(prices)>1:
            minPrice=prices[0]
            for price in prices:
                profitMax = max(profitMax, price - minPrice)
                minPrice = min(minPrice, price)
        return profitMax
